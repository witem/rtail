{
  "name": "r-tail",
  "version": "2.1.3",
  "description": "Terminal output to the browser in seconds, using UNIX pipes",
  "keywords": [
    "tail",
    "log",
    "logs",
    "logio",
    "logging",
    "udp"
  ],
  "author": "Artem Kuzmenko <witem.artem@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:witem/rtail.git"
  },
  "bugs": {
    "url": "https://github.com/witem/rtail/issues"
  },
  "engines": {
    "node": ">=8"
  },
  "bin": {
    "rtail": "./cli/rtail-client.js",
    "rtail-server": "./cli/rtail-server.js"
  },
  "scripts": {
    "start": "DEBUG=rtail:*,api:* ./cli/rtail-server.js",
    "clean": "rm -rf dist",
    "dev": "npm run clean && webpack-dev-server -d --open --progress",
    "build": "npm run clean && NODE_ENV=production webpack -p --hide-modules --progress",
    "lint": "./node_modules/.bin/eslint . --ext js,vue",
    "prepublishOnly": "npm run lint && npm test && npm run build",
    "test": "NODE_ENV=test ./node_modules/.bin/mocha"
  },
  "browserslist": "> 0.25%, not dead",
  "dependencies": {
    "chrono-node": "^1.4.6",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "json5": "^2.1.3",
    "lodash": "^4.17.15",
    "moniker": "^0.1.2",
    "request": "^2.88.2",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "split": "^1.0.0",
    "strip-ansi": "^6.0.0",
    "through2-map": "^3.0.0",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/runtime": "^7.10.2",
    "ansi_up": "^4.0.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "css-loader": "^3.5.3",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-html": "^6.0.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^6.2.2",
    "fecha": "^4.2.0",
    "file-loader": "^6.0.0",
    "highlight.js": "^10.0.3",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "mocha": "^7.2.0",
    "npm": "^6.14.5",
    "vue": "^2.6.11",
    "vue-loader": "^15.9.2",
    "vue-material": "^1.0.0-beta-14",
    "vue-router": "^3.3.2",
    "vue-socket.io": "^3.0.9",
    "vue-template-compiler": "^2.6.11",
    "vuescroll": "^4.15.1",
    "vuex": "^3.4.0",
    "vuex-persistedstate": "^3.0.1",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  }
}
